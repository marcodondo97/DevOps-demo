define host {
    host_name              {{ HostName }}
    address                {{ HostAddress }}
    max_check_attempts     3
    check_period           24x7
    check_command          check_http!-p 5000
}

define service {
    use                   generic-service
    host_name              {{ HostName }}
    service_description    HTTP
    check_command          check_http!-p 5000
}
